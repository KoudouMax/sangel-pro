{# Generic Tailwind auth shell #}
{% set _attrs = (attributes is defined ? attributes : create_attribute())
  .removeClass('min-h-screen','flex','flex-col','bg-sangel-ice')
  .addClass('auth-shell', page_modifier is defined ? page_modifier : '') %}

{% set _visual_badge = visual_badge is defined ? visual_badge : 'Espace client Sangel Pro'|t %}
{% set _visual_heading = visual_heading is defined ? visual_heading : 'Pilotez votre activité surgelée en toute sérénité'|t %}
{% set _visual_description = visual_description is defined ? visual_description : 'Centralisez vos commandes, suivez vos livraisons et échangez avec nos experts en un seul espace professionnel.'|t %}
{% set _features = visual_features is defined ? visual_features : [
  {
    title: 'Catalogue en temps réel'|t,
    description: 'Accédez aux nouveautés, aux disponibilités et à vos tarifs négociés en quelques clics.'|t
  },
  {
    title: 'Commandes orchestrées'|t,
    description: 'Gérez vos paniers récurrents, adaptez vos volumes et planifiez vos livraisons simplement.'|t
  },
  {
    title: 'Support dédié'|t,
    description: 'Nos spécialistes Sangel suivent vos dossiers et répondent rapidement à chaque demande.'|t
  }
] %}
{% set _features = _features is iterable ? _features : [] %}
{% set _stat = visual_stat is defined ? visual_stat : {
  highlight: '24h/24',
  label: 'Portail accessible'|t,
  text: 'Passez vos commandes et suivez vos livraisons à tout moment, où que vous soyez.'|t
} %}
{% set _has_stat = _stat.highlight is defined or _stat.label is defined or _stat.text is defined %}
{% set _has_visual_side = (visual_logo is defined and visual_logo) or _has_stat %}

<div{{_attrs}}>
	<div class="auth-shell__visual" role="img" aria-label="{{ visual_aria_label|default('Univers produits surgelés Sangel Pro'|t) }}">
		<div class="auth-shell__flakes">
			<span class="auth-shell__flake auth-shell__flake--bottom">❄</span>
			<span class="auth-shell__flake auth-shell__flake--top">❄</span>
		</div>
		<div class="auth-shell__visual-inner">
			<div class="auth-shell__visual-copy">
				{% if _visual_badge %}
					<span class="auth-shell__visual-badge">{{ _visual_badge }}</span>
				{% elseif visual_caption %}
					<span class="auth-shell__visual-badge">{{ visual_caption }}</span>
				{% endif %}

				{% if _visual_heading %}
					<h2 class="auth-shell__visual-title">{{ _visual_heading }}</h2>
				{% endif %}

				{% if _visual_description %}
					<p class="auth-shell__visual-text">{{ _visual_description }}</p>
				{% endif %}

				{% if _features %}
					<ul class="auth-shell__feature-list">
						{% for feature in _features %}
							<li class="auth-shell__feature">
								<span class="auth-shell__feature-dot" aria-hidden="true"></span>
								<div class="auth-shell__feature-body">
									{% if feature.title %}
										<span class="auth-shell__feature-title">{{ feature.title }}</span>
									{% endif %}
									{% if feature.description %}
										<span class="auth-shell__feature-desc">{{ feature.description }}</span>
									{% endif %}
								</div>
							</li>
						{% endfor %}
					</ul>
				{% endif %}
			</div>

			{% if _has_visual_side %}
				<div class="auth-shell__visual-side">
					{% if visual_logo %}
						<div class="auth-shell__visual-logo">{{ visual_logo }}</div>
					{% endif %}

					{% if _has_stat %}
						<div class="auth-shell__stat-card">
							{% if _stat.highlight %}
								<span class="auth-shell__stat-highlight">{{ _stat.highlight }}</span>
							{% endif %}
							{% if _stat.label %}
								<span class="auth-shell__stat-label">{{ _stat.label }}</span>
							{% endif %}
							{% if _stat.text %}
								<p class="auth-shell__stat-text">{{ _stat.text }}</p>
							{% endif %}
						</div>
					{% endif %}
				</div>
			{% endif %}
		</div>
	</div>

	<div class="auth-shell__panel {{ shell_modifier|default('') }}">
		<div class="auth-shell__content">
			<div class="auth-shell__card">
				<div class="auth-shell__logo h-14 w-auto">
					{% if directory and brand_logo_path is not defined %}


						<svg class="w-40 h-20" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="395" height="124" viewbox="0 0 395 124">
							<defs>
								<pattern id="pattern" preserveaspectratio="none" width="100%" height="100%" viewbox="0 0 874 401">
									<image width="874" height="401" xlink:href="data:image/png;base64,"/>
								</pattern>
							</defs>
							<g id="Groupe_2785" data-name="Groupe 2785" transform="translate(-76 -73)">
								<rect id="logo" width="269" height="124" transform="translate(76 73)" fill="url(#pattern)"/>
								<text id="PRO" transform="translate(351 163)" fill="#5578ab" font-size="55" font-family="Arial-BoldMT, Arial" font-weight="700">
									<tspan x="0" y="0">PRO</tspan>
								</text>
							</g>
						</svg>

					{% elseif brand_logo_path is defined %}
						<img class="h-14 w-auto" src="{{ brand_logo_path }}" alt="{{ brand_alt|default('Sangel Pro'|t) }}">
					{% else %}
						<span class="text-2xl font-extrabold tracking-wide">{{ brand_text|default('Sangel Pro') }}</span>
					{% endif %}
				</div>

				{% if title %}
					<h1 class="auth-shell__title">{{ title }}</h1>
				{% endif %}

				{% if subtitle %}
					<p class="auth-shell__subtitle">{{ subtitle }}</p>
				{% endif %}

				{% if messages %}
					<div class="auth-shell__messages">
						{{ messages }}
					</div>
				{% endif %}

				<div class="auth-shell__form">
					{{ form|default(page.content) }}
				</div>

				{% set _has_links = primary_link_route is defined and primary_link_route or (links is defined and links) %}
				{% if _has_links %}
					<div class="auth-shell__links">
						{% if primary_link_route is defined and primary_link_route %}
							<a class="auth-shell__link" href="{{ path(primary_link_route) }}">{{ primary_link_text|default('') }}</a>
						{% endif %}
						{% if links is defined and links %}
							{% for link in links %}
								<a class="auth-shell__link" href="{{ link.url }}">{{ link.text }}</a>
							{% endfor %}
						{% endif %}
					</div>
				{% endif %}

				{% if meta %}
					<div class="auth-shell__meta">{{ meta }}</div>
				{% endif %}
			</div>
		</div>
	</div>
</div>

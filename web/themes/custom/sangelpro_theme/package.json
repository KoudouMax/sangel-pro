{
  "name": "sangelpro-theme",
  "version": "1.0.0",
  "description": "Tailwind build tooling for the SangelPro Drupal theme.",
  "scripts": {
    "dev": "tailwindcss -i ./assets/css/tailwind.css -o ./build/css/style.css --watch",
    "build": "tailwindcss -i ./assets/css/tailwind.css -o ./build/css/style.css --minify"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "tailwindcss": "^3.4.1"
  }
}
